template header
age
name
className

package rulesTwo.isDrt;
import com.pojo.Person;

template "cheesefans"

rule "Cheese fans_@{row.rowNumber}"
no-loop true
    when
       $p: Person(age == @{age},name=="@{name}")
    then
        $p.setClassName("@{className}");
        update($p);
end
end template
